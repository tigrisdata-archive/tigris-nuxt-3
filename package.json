{
  "name": "tigris-nuxt-3",
  "private": true,
  "scripts": {
    "tigris:init": "TIGRIS_PROJECT=\"${npm_config_project:=tigris-nuxt-3}\" && tigris create project $TIGRIS_PROJECT -o . --create-env-file",
    "tigris:seed": "TIGRIS_PROJECT=\"${npm_config_project:=tigris-nuxt-3}\" && npx ts-node ./scripts/seed.ts --create-dirs && cat seed/prizes.json | tigris import prizes -p $TIGRIS_PROJECT",
    "tigris:setup": "npx ts-node ./scripts/setup.ts",
    "prebuild": "npm run tigris:setup",
    "build": "nuxt build",
    "predev": "npm run tigris:setup",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "@tigrisdata/tigris-cli": "^1.0.0-beta.32",
    "@types/node": "^18",
    "nuxt": "^3.5.2"
  },
  "dependencies": {
    "@tigrisdata/core": "^1.2.4",
    "reflect-metadata": "^0.1.13"
  }
}
